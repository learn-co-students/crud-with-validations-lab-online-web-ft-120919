<div class="table">
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Release Year</th>
        <th>Released?</th>
        <th>Artist</th>
        <th>Genre</th>
      </tr>
    </thead>
    <tbody>
      <% @songs.each do |song| %>
        <tr>
          <td><%= song.title %></td>
          <td><%= song.release_year %></td>
          <td><%= song.released ? "Yes" : "No" %></td>
          <td><%= song.artist_name %></td>
          <td><%= song.genre %></td>
        </tr>
      <% end%>
    </tbody>
  </table>
</div>