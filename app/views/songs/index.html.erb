<% @songs.each.with_index(1) do |song, i| %>
<tr>
  <div><%= i %></div>
  <div><%= song.title %></div>
  <div><%= song.released %></div>
  <div><%= song.release_year %></div>
  <div><%= song.artist_name %></div>
  <div><%= song.genre %></div>
  <%= link_to "Delete", song, method: :delete, data: { confirm: "Really?" } %>
</tr>
<% end %>