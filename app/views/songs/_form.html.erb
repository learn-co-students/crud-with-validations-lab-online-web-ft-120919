
<%= form_for @song do |f| %>
  <div class="field">
    <h3><%= f.label :title %></h3>
    <%= f.text_field :title%>
    <br>
  </div>
  <br>
  <div class="field">
    <%= f.label "released", "Released?" %>
    <p>Check if its been released!</p>
    <%= f.check_box :released, {}, true, false %>
    <br>
  </div>
  <br>
  <div class="field">
  <%= f.label :release_year %>
  <br>
  <%= f.text_field :release_year %>
  </div>
  <br>
<div class="field">
  <%= f.label :artist_name %>
  <br>
  <%= f.text_field :artist_name %>
</div>
<br>
<div class="field">
  <%= f.label :genre %>
  <br>
  <%= f.text_field :genre %>
</div>
<br>
  <%= f.submit %>
  <%end%>